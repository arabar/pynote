msgstr "Neue Revisionsnummer"

#: pynote/__main__.py:52
msgid "old revision number"
msgstr "Alte Revisionsnummer"

#: pynote/__main__.py:54
msgid "do not use colors"
msgstr "Keine Farben benutzen"

#: pynote/__main__.py:57
msgid "initialize pynote"
msgstr "pynote initialisieren"

#: pynote/__main__.py:60
msgid "only create a new config string and print it out"
msgstr "Nur eine neue Konfiguration erstellen und ausgeben"

#: pynote/__main__.py:63
msgid "remove an existing rc-file and init pynote"
msgstr "Eine bereits vorhandene rc-Datei löschen und pynote initialisieren"

#: pynote/__main__.py:70
msgid "show version"
msgstr "Versionsinformationen ausgeben"

#: pynote/__main__.py:96
msgid "Error: from_rev must not be smaller than to_rev!"
msgstr "Fehler: from_rev muss kleiner sein als to_rev!"

#: pynote/command.py:26
msgid "You have no data in pynote... :-)"
msgstr "Du hast keine Daten in pynote... :-)"

#: pynote/command.py:115
msgid "You have not changed anything!"
msgstr "Du hast nichts verändert!"

#: pynote/command.py:116
msgid "No new revision has been created!"
msgstr "Keine neue Revision wurde erstellt."

#: pynote/command.py:153
msgid "You have no data in pynote trash... :-)"
msgstr "Du hast keine Daten im Papierkorb von pynote... :-)"

#: pynote/command.py:219
msgid "Error: Maybe the revisions do not exist?"
msgstr "Fehler: Vielleicht existieren die gesuchten Revisionen nicht?"

#: pynote/command.py:250
msgid "There are {} revisions of '{}':"
msgstr "Es gibt {} Revisionen von '{}':"

#: pynote/config.py:25
msgid "It's something wrong with your 'noterc' in {}."
msgstr "Etwas stimmt nicht mit deiner noterc in {}."

#: pynote/config.py:26
msgid "Try running 'note-init' to create a valid 'noterc'!"
msgstr "Versuche mit Hilfe von 'note init' eine valide 'noterc' zu erstellen!"

#: pynote/helper.py:33
msgid "Error: This note does not exist!"
msgstr "Fehler: Diese Notiz existiert nicht!"

#: pynote/helper.py:41
msgid "Lexer not found!"
msgstr "Lexer nicht gefunden!"

#: pynote/init.py:13
msgid "Where do you want to store your notes [default: ~/.note]?"
msgstr "Wo willst du deine Notizen speichern [default: ~/.note]?"

#: pynote/init.py:19
msgid "Enter your prefered dateformat [default: Y-m-d H:M]!"
msgstr "Gib dein bevorzugtes Datumsformat ein [default: Y-m-d H:M]!"

#: pynote/init.py:25
msgid "What is your prefered editor [default: {}]?"
msgstr "Was ist dein bevorzugter Editor [default: {}]?"

#: pynote/init.py:35
msgid "Write this to your '~/.noterc':"
msgstr "Schreibe in deine '~/.noterc':"

#: pynote/init.py:47
msgid "The following config has been written to '~/.noterc':"
msgstr "Folgendes wurde in deine '~/.noterc' geschrieben:"

#: pynote/init.py:50
msgid "A '~/.noterc' already exists, use '--force' to overwrite!"
msgstr "Eine '~/.noterc' existiert bereits, benutze --force zum überschreiben!"

#: pynote/init.py:56
msgid "Directory '{}' already exists!"
msgstr "Das Verzeichnis '{}' existiert bereits!"

