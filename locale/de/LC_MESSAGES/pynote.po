#: pynote/__main__.py:13
msgid "show a table with all active notes"
msgstr "Tabelle mit allen aktiven Notizen ausgeben"

#: pynote/__main__.py:17
msgid "show a specific note"
msgstr "Notiz anzeigen"

#: pynote/__main__.py:18 pynote/__main__.py:30 pynote/__main__.py:35
#: pynote/__main__.py:49
msgid "integer key which is shown in the table"
msgstr "Key, der in der Tabelle angezeigt wird"

#: pynote/__main__.py:21
msgid "do not show header data"
msgstr "Keine Header Daten ausgeben"

#: pynote/__main__.py:22
msgid "specify synthax highlighting"
msgstr "Synthax Highlighting benutzen"

#: pynote/__main__.py:25
msgid "create a new note"
msgstr "Neue Notiz erstellen"

#: pynote/__main__.py:29
msgid "edit a note"
msgstr "Notiz bearbeiten"

#: pynote/__main__.py:34
msgid "move a note to trash"
msgstr "Notiz in den Papierkorb verschieben"

#: pynote/__main__.py:39
msgid "show a table with all deleted notes"
msgstr "Tabelle mit gelöschten Notizen anzeigen"

#: pynote/__main__.py:43
msgid "restore a deleted note"
msgstr "Gelöschte Notiz wiederherstellen"

#: pynote/__main__.py:44
msgid "integer key which is shown in the trash table"
msgstr "Key, der in der Papierkorb-Tabelle angezeigt wird"

#: pynote/__main__.py:48
msgid "compare two notes"
msgstr "Zwei Notizen vergleichen"

#: pynote/__main__.py:51
